<h1>New photo</h1>
<% form_for(:photo, @photo, :url => {:action=>'create'}, :html=>{:multipart=>true}) do |f| %>
 <%= f.error_messages %>
 <p>
  <%= f.label :image_file %><br />
  <%= f.file_field :image_file %>
 </p>
 <p>
  <%= f.label :description %><br />
  <%= f.text_field :description %>
 </p>
 <p>
  <%= f.submit "Create" %>
 </p>
<% end %>
<%= link_to 'Back', photos_path %>
